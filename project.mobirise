type == 'color'\">\n        <input type=\"color\" title=\"Color 2\" name=\"color2\" value=\"#21b968\" condition=\"bg.type=='color' && gradientBg\" selected>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <div class=\"row align-items-start\">\n            <div class=\"col-md-6 justify-content-center\" mbr-if=\"itemsCount > 0\" mbr-class=\"{\n                 'col-lg-4': itemsCount == 3,\n                 'col-lg-6': itemsCount == 2,\n                 'col-lg-3': itemsCount == 4}\">\n                <div class=\"plan\">\n                    <div class=\"mbr-overlay\"></div>\n                    <div class=\"plan-header text-center\">\n                        <h3 class=\"plan-title mbr-white mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".plan-title\">\n                            Mixing</h3>\n                        <h4 class=\"plan-subtitle mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".plan-subtitle\">\n                            A beautiful. simple website\n                        </h4>\n                        <div class=\"plan-price\">\n                            <span mbr-text class=\"price-figure mbr-white mbr-fonts-style\" mbr-theme-style=\"display-1\" data-app-selector=\".price-figure\">Startingat 20$</span>\n                            <div mbr-text class=\"price-term mbr-white\" mbr-if=\"showPriceTerm\">\n                                <div class=\"term mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".price-term\">per every year</div>\n                            </div>\n                            <p class=\"mbr-text mbr-fonts-style \" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText\"></p>\n                        </div>\n                    </div>\n                    <div class=\"plan-body\">\n                        <div class=\"plan-list align-center\">\n                            <ul mbr-text class=\"list-group list-group-flush mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                <li class=\"list-group-item\">\n                                    Low cost for only beat</li>\n                                <li class=\"list-group-item\">Prices depends on number of Stems&nbsp;</li><br><li class=\"list-group-item\">top of the line hardwar and software</li><br><li class=\"list-group-item\">Free Mastering With all Services</li>\n                            </ul>\n                        </div>\n                        <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn text-center pt-4\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a href=\"page4.html\" class=\"btn btn-bgr btn-warning\">Sample</a> <a href=\"page1.html\" class=\"btn btn-bgr btn-warning\">Contact</a></div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"col-md-6 justify-content-center\" mbr-if=\"itemsCount > 1\" mbr-class=\"{\n                 'col-lg-4': itemsCount == 3,\n                 'col-lg-6': itemsCount == 2,\n                 'col-lg-3': itemsCount == 4}\">\n                <div class=\"plan\">\n                    <div class=\"mbr-overlay\"></div>\n                    <div class=\"plan-header text-center\">\n                        <h3 class=\"plan-title mbr-white 